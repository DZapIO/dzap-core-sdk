{
  "arrowParens": "always",
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "singleQuote": true,
  "trailingComma": "all",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ["*.json", ".eslintrc", ".prettierrc"],
      "options": {
        "trailingComma": "none"
      }
    }
  ]
}
